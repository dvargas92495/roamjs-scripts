{
  "name": "roamjs-scripts",
  "description": "Description for roamjs-scripts",
  "version": "0.13.1",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prebuild": "cross-env NODE_ENV=test npm t",
    "build": "esbuild src/index.ts --outfile=dist/index.js --bundle --platform=node --external:emitter --external:pnpapi --external:watchpack --external:webpack-dev-server --external:terser-webpack-plugin --external:esbuild",
    "postbuild": "node postbuild.js",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint . --ext .ts",
    "prepublishOnly": "npm run build",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push origin main && git push --tags",
    "pretest": "npm run lint",
    "test": "jest --config jestconfig.json",
    "posttest": "rimraf build"
  },
  "license": "MIT",
  "files": [
    "/dist"
  ],
  "bin": {
    "roamjs-scripts": "./dist/index.js"
  },
  "devDependencies": {
    "@types/cross-spawn": "^6.0.2",
    "@types/git-repo-name": "^1.0.0",
    "@types/git-user-name": "^2.0.0",
    "@types/jest": "^26.0.23",
    "@types/mime-types": "^2.1.0",
    "@types/mini-css-extract-plugin": "^1.4.2",
    "@types/react-dev-utils": "^9.0.5",
    "@types/rimraf": "^3.0.0",
    "@types/terser-webpack-plugin": "^5.0.3",
    "@types/webpack-dev-server": "^3.11.3",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.25.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "process": "^0.11.10",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.5",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@babel/core": "^7.14.3",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-react": "^7.13.13",
    "aws-sdk": "^2.904.0",
    "axios": "^0.21.1",
    "babel-loader": "^8.2.2",
    "cross-spawn": "^7.0.3",
    "css-loader": "^5.2.5",
    "dotenv": "^8.4.0",
    "esbuild": "^0.12.22",
    "file-loader": "^6.2.0",
    "git-repo-name": "^1.0.1",
    "git-user-name": "^2.0.0",
    "ignore-loader": "^0.1.2",
    "jszip": "^3.7.1",
    "mime-types": "^2.1.30",
    "nearley-loader": "^2.0.0",
    "node-polyfill-webpack-plugin": "^1.1.2",
    "shebang-loader": "0.0.1",
    "source-map-loader": "^3.0.0",
    "style-loader": "^2.0.0",
    "svg-react-loader": "^0.4.6",
    "terser-webpack-plugin": "^5.1.3",
    "ts-loader": "^9.2.1",
    "tweetsodium": "0.0.5",
